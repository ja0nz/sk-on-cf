{
  "name": "ja_nz",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "sk": "svelte-kit",
    "start": "run-p 'cssgen dev' 'sk dev'",
    "build": "run-s 'cssgen build' 'sk build'",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .prettierignore .",
    "format": "prettier --write --plugin-search-dir=. .",
    "cssgen": "node ./css-gen.js"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@sveltejs/adapter-static": "^1.0.0-next.29",
    "@sveltejs/kit": "^1.0.0-next.321",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "autoprefixer": "^10.4.5",
    "chokidar": "^3.5.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.47.0",
    "svelte-check": "^2.7.0",
    "svelte-preprocess": "^4.10.6",
    "tailwindcss": "^3.0.24",
    "tslib": "^2.4.0",
    "typescript": "~4.6.3"
  },
  "packageManager": "yarn@3.2.0",
  "dependencies": {
    "@octokit/graphql": "^4.8.0",
    "@thi.ng/compose": "^2.1.5",
    "@thi.ng/paths": "^5.1.5"
  }
}
